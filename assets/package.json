		"description": "add laravel mix to codeigniter",
		"main": "index.js",
		"scripts": {
				"dev": "npm run development",
				"development": "webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
				"watch": "npm run development -- --watch",
				"hot": "webpack --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
				"prod": "npm run production",
				"production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
		},
